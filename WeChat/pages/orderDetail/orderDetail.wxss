.navbar {
 display: flex;
 background: #fff;
}

.none_tab {
 position: relative;
 top: 20rpx;
 color: #999;
 font-size: 32rpx;
}

/* 默认选中菜单 */

.navbar .item {
 position: relative;
 flex: auto;
 font-size: 27rpx;
 width: 100rpx;
 text-align: center;
 line-height: 80rpx;
 color: #333;
 border-bottom: 6rpx solid #ddd;
}

/* 选中菜单样式 */

.navbar .item.active {
 color: #333;
 border-bottom: 6rpx solid #d66058;
}

/* 字体 */

.navbar .item.active:after {
 content: "";
 display: block;
 position: absolute;
 bottom: 0;
 left: 0;
 right: 0;
 height: 4rpx;
}

.cont_count {
 padding-top: 6rpx;
 position: relative;
 width: 100%;
 text-align: center;
 /* height: 620rpx; *//* border-bottom: 6rpx solid #ddd; */
 margin-bottom: 6rpx;
 background-color: #fff;
}
.seria {
 position: relative;
 display: inline-block;
 width: 90%;
 height: 170rpx;
 line-height: 70rpx;
 /* background-color: yellow; */
 border-bottom: 1rpx solid #d4d4d4;
}

/* 订单lab */

.seriac_name {
 position: absolute;
 font-size: 28rpx;
 color: #333;
 left: 0rpx;
}

.seriac_name text {
 margin-left: 15rpx;
}

/* 订单状态 */

.seriac_start {
 position: absolute;
 right: 0rpx;
 color: #333;
 font-size: 28rpx;
}

.seriac_start text {
 margin-left: 40rpx;
}

.seriac_orderTime {
 float: left;
 font-size: 28rpx;
 color: #333;
 left: 0rpx;
 margin-top: 25px;
}

.seriac_orderTime text {
 margin-left: 15rpx;
}

.seriac_amount {
 position: absolute;
 right: 0rpx;
 color: #333;
 font-size: 28rpx;
 margin-top: 25px;
}

.seriac_amount text {
 margin-left: 40rpx;
}

.seriac_remark {
 float: left;
 font-size: 28rpx;
 color: #333;
 left: 0rpx;
 margin-top: 50px;
 margin-left: -168px;
}

.seriac_remark text {
 margin-left: 15rpx;
}

.seriac_detail {
 position: absolute;
 right: 0rpx;
 color: #333;
 font-size: 28rpx;
 margin-top: 60px;
}